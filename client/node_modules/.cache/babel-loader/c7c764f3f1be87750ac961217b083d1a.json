l\" min=\"0\" v-model.number=\"investimentoInicial\">\r\n            <p>O investimento inicial é: {{ investimentoInicial }}</p>\r\n        </div>\r\n        <button class=\"btn btn-primary\"><span class=\"bi-check-circle\"></span> Simular Futuro</button>\r\n        <p>O valor de notSubmitted é: {{ notSubmitted }}</p>\r\n     </form>\r\n    </div>\r\n    <div v-else>\r\n        <!--<p>O valor do chartData considerando o rendimento do CDB em julho é: {{ chartData['CDB'] }}</p>\r\n        <p v-for=\"(value, key) in chartData\" :key=\"key\">\r\n            <strong>\r\n              {{ key }}   - {{ value }}\r\n            </strong>\r\n        </p>-->\r\n        <div class=\"chart-container\">\r\n          <ChartComponent :chart-data=\"chartData\" /><!--:chart-options=\"chartOptions\"-->\r\n        </div>\r\n        <div id=\"nova-simulacao\">\r\n            <button class=\"btn btn-primary\" @click=\"notSubmitted = !notSubmitted\">Nova Simulação</button>\r\n        </div>\r\n     </div>\r\n     \r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport ChartComponent from '../components/ChartComponent.vue'\r\nimport DataService from \"../../services/DataService\"\r\n\r\n//const chartInstance = this.$refs.bar.chart\r\n//this.$refs.bar.updateChart()\r\n\r\nexport default {\r\n    name: \"FuturoComponent\",\r\n    components: { ChartComponent },\r\n    data() {\r\n        return {\r\n            checkedInvestimentos: [],\r\n            tempoInvestimento: \"\",\r\n            investimentoInicial: \"\",\r\n            montante:\"\",\r\n            chartData: {\r\n                        labels: [],\r\n                        datasets: [\r\n                                    {\r\n                                      label: 'Data One',\r\n                                      backgroundColor: '#f87979',\r\n                                      dados: [40, 20, 12]\r\n                                    }\r\n                                  ]\r\n                        },\r\n            //chartOptions:,\r\n            notSubmitted: true\r\n        };\r\n    },\r\n    methods: {\r\n        registerInvestimento: function () {\r\n\r\n           let data = {\r\n              tempoInvestimento: this.tempoInvestimento,\r\n              investimentoInicial: this.investimentoInicial,\r\n              checkedInvestimentos: this.checkedInvestimentos\r\n            };\r\n\r\n\r\n\r\n            DataService.create(data)\r\n            .then(response => {\r\n              this.showResultados(response.data);\r\n             })\r\n          .catch(error => {\r\n          console.log(error.message);\r\n  });\r\n\r\n        },\r\n\r\n        showResultados: function(data){\r\n                this.montante = data;\r\n                this.chartData.labels = this.checkedInvestimentos;\r\n                console.log(this.chartData.datasets[2])\r\n              /* for(const key in data){\r\n                  this.chartData.labels.push(key);\r\n                 // this.chartData.datasets['dados'].push(data[key]);\r\n                }*/\r\n              this.notSubmitted = false;\r\n        }\r\n    },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* Chrome, Safari, Edge, Opera */\r\ninput::-webkit-outer-spin-button,\r\ninput::-webkit-inner-spin-button {\r\n  -webkit-appearance: none;\r\n  margin: 0;\r\n}\r\n\r\n/* Firefox */\r\ninput[type=number] {\r\n  -moz-appearance: textfield;\r\n}\r\n\r\n.chart-container {\r\n  max-height: 380px;\r\n  max-width: 400px;\r\n  align-items: center;\r\n  display: flex;\r\n}\r\n\r\n#nova-simulacao{\r\n  margin-top: 10px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}