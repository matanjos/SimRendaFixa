{"ast":null,"code":"//import ChartComponent from '../components/ChartComponent.vue'\nimport DataService from \"../../services/DataService\";\nimport BarChart from \"./BarChart.vue\"; //const chartInstance = this.$refs.bar.chart\n//this.$refs.bar.updateChart()\n\nexport default {\n  name: \"FuturoComponent\",\n  components: {\n    BarChart\n  },\n\n  data() {\n    return {\n      checkedInvestimentos: [],\n      tempoInvestimento: \"\",\n      investimentoInicial: \"\",\n      montante: \"\",\n      chartData: {\n        labels: [],\n        datasets: [{\n          label: 'Data One',\n          backgroundColor: '#f87979',\n          dados: []\n        }]\n      },\n      //chartOptions:,\n      notSubmitted: true,\n      loaded: false\n    };\n  },\n\n  methods: {\n    registerInvestimento: function () {\n      this.chartData.datasets[0].dados = [];\n      let data = {\n        tempoInvestimento: this.tempoInvestimento,\n        investimentoInicial: this.investimentoInicial,\n        checkedInvestimentos: this.checkedInvestimentos\n      };\n      DataService.create(data).then(response => {\n        //for(const key in response.data){\n        //this.chartData.datasets[0].dados.push(response.data[key]);\n        this.chartData.datasets[0].dados = [1, 2, 3, 4, 5]; //  }\n\n        this.chartData.labels = this.checkedInvestimentos;\n        this.showResultados(response.data);\n      }).catch(error => {\n        console.log(error.message);\n      });\n    },\n    showResultados: function (data) {\n      this.montante = data;\n      console.log(this.chartData.datasets[0].dados);\n      this.notSubmitted = false;\n      this.loaded = true;\n    }\n  }\n};","map":{"version":3,"mappings":"AA6EA;AACA,OAAOA,WAAP,MAAwB,4BAAxB;AACA,OAAOC,QAAP,MAAqB,gBAArB,C,CAEA;AACA;;AAEA,eAAe;EACXC,IAAI,EAAE,iBADK;EAEXC,UAAU,EAAE;IAAGF;EAAH,CAFD;;EAGXG,IAAI,GAAG;IACH,OAAO;MACHC,oBAAoB,EAAE,EADnB;MAEHC,iBAAiB,EAAE,EAFhB;MAGHC,mBAAmB,EAAE,EAHlB;MAIHC,QAAQ,EAAC,EAJN;MAKHC,SAAS,EAAE;QACCC,MAAM,EAA